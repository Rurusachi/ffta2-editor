<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.ruru.ffta2editor.TextController">
   <children>
      <SplitPane dividerPositions="0.2, 0.4" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <items>
            <ScrollPane fitToHeight="true" fitToWidth="true" SplitPane.resizableWithParent="false">
               <content>
                  <ListView fx:id="messageList" />
               </content>
            </ScrollPane>
            <ScrollPane fitToHeight="true" fitToWidth="true" layoutX="10.0" layoutY="10.0" SplitPane.resizableWithParent="false">
               <content>
                  <ListView fx:id="stringList" />
               </content>
            </ScrollPane>
            <AnchorPane>
               <children>
                  <TextArea fx:id="stringArea" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
               </children>
            </AnchorPane>
         </items>
      </SplitPane>
   </children>
</AnchorPane>
