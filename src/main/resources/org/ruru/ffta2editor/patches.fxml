<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<AnchorPane xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.ruru.ffta2editor.PatchesController">
   <children>
      <HBox alignment="CENTER_LEFT" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <VBox alignment="TOP_CENTER" prefHeight="200.0" spacing="20.0">
               <padding>
                  <Insets top="20.0" />
               </padding>
               <children>
                  <Button fx:id="animationFix" mnemonicParsing="false" onAction="#applyAnimationFix" text="Animation Fix (irreversible)" />
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
</AnchorPane>
